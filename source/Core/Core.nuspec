    <tags>codefirst ucommerce</tags>
  </metadata>
  <files>
    <file src="..\..\nuget\web.config.install.xdt" target="content" />
    <file src="..\..\nuget\web.config.uninstall.xdt" target="content" />
  </files>
</package>